generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id      Int     @id @default(autoincrement())
  title   String
  isDone  Boolean @default(false)
  isDelete Boolean @default(false)
}
